<h1>Calc#view</h1>
<p>Find me in app/views/calc/view.html.erb</p>

<table border="1">
<caption>Table of result</caption>
   <tr>
    <th>i</th>
    <th>number</th>
   </tr>
	<% for i in 0..@result.length-1 %> 
		<tr><td><%=i+1%></td><td><%=@result[i]%></td></tr>   
	<%end%> 

</table>

<%= link_to "Repeat calculation", :calc_input %>
